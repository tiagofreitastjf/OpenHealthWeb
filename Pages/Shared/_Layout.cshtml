    <script src="https://kit.fontawesome.com/00dbf79896.js" crossorigin="anonymous"></script>
</head>
<body style="overflow-y: scroll">
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-info text-light border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand text-light" style="font-weight: 800;" asp-area="" asp-page="/Index">Open Health</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1 text-light">
                        <li class="nav-item">
                            <a class="nav-link text-light" asp-area="" asp-page="/Index"><i class="fas fa-home"></i></a>
                        </li>
                        @*<li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Prontuário</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Solicitacao">Solicitações</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/EnviarExame">Enviar Exames</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/MeusExames">Exames</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Conta</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-page="/Privacy">Ajuda</a>
                        </li>*@
                    </ul>
                    <a type="button" asp-area="" asp-page="/Logout" class="nav-link btn bg-transparent text-light">Sair&ensp;<i class="fas fa-sign-out-alt"></i></a>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <div class="footer text-center font-italic p-3 border-top">
        <a class="text-black-50" style="text-decoration: none" asp-area="" asp-page="/Index">Projeto Open Health 2021</a> 
    </div>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>


